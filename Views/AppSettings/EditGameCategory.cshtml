@model GameCategory

@{
    ViewData["Title"] = "EditGameCategory";
}
<div class="card">
    <div class="card-header">
        <h6>Edit GameCategory</h6>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-md-4">
                <form asp-action="Edit">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="Id" />
                    <div class="form-group">
                        <label asp-for="GameCategoryName" class="control-label"></label>
                        <input asp-for="GameCategoryName" class="form-control" />
                        <span asp-validation-for="GameCategoryName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Save" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="card-footer">
        <a class="btn btn-sm btn-outline-success" asp-action="IndexSearchGameCategory">Back to GameCattagories</a>
    </div>
</div>