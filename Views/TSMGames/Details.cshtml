@model TSMGame

@{
    ViewData["Title"] = "DetailsTSMGame";
}

<h6>Details for Game: @Html.ViewData.Model.Id </h6>

<div>
    <h6>TSMGame: @Html.ViewData.Model.GameNumber</h6>
    
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.GameDateTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.GameDateTime)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WeekDay)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WeekDay)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.GameNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.GameNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Round)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Round)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HomeTeam)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HomeTeam)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AwayTeam)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AwayTeam)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Arena)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Arena)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Series)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Series)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HD1)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HD1)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HD2)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HD2)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Supervisor)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Supervisor)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateChanged)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateChanged)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ChangedBy)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ChangedBy)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TSMGameStatus)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TSMGameStatus.TSMGameStatusName)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-sm btn-success" asp-controller="TSMGames" asp-action="Edit" asp-route-id="@Model.Id">Edit</a>
    <a class="btn btn-sm btn-success" asp-controller="TSMGames" asp-action="IndexSearch">Back to TSMGames</a>
</div>
